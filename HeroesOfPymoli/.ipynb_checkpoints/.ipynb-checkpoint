   "source": [
    "#Percentage and Count of Male Players\n",
    "#Percentage and Count of Female Players\n",
    "#Percentage and Count of Other / Non-Disclosed\n",
    "\n",
    "\n",
    "gender_demo_df = purchase_data[\"Gender\"].value_counts()\n",
    "gender_demographics_df = pd.DataFrame(gender_demo_df)\n",
    "\n",
    "gender_percent = (purchase_data[\"Gender\"].value_counts()/ (purchase_data[\"Gender\"].count()))*100\n",
    "gender_demographics_df[\"Percent of Players\"] = gender_percent.map(\"{:,.2f}%\".format)\n",
    "Gender_df = pd.DataFrame(gender_demographics_df)\n",
    "\n",
    "gender_DF = Gender_df.rename(columns={\"Gender\": \"Total Count\"})\n",
    "gender_DF\n",
    "\n",
    "\n",
